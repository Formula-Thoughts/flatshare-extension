{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Flatini",
  "action": {
    "default_popup": "js/index.html",
    "default_title": "Flatini",
    "default_icon": {
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png",
      "128": "icons/128.png",
      "256": "icons/256.png",
      "512": "icons/512.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "permissions": ["sidePanel", "tabs"],
  "side_panel": {
    "default_path": "js/index.html"
  }
}
